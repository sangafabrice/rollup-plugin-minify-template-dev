{
  "name": "rollup-plugin-minify-template-dev",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "scripts": {
    "commit:lib": "npm run commit:lib --workspace=src",
    "commit:src": "npm run commit --workspace=src",
    "push": "git push --all --recurse-submodules=on-demand",
    "package:update": "npx --no --yes npm-check-updates --upgrade --workspaces && npm install",
    "flow:stop": "flow stop; while (@(Stop-Process -Name flow -Force -PassThru -ErrorAction Ignore).Length) { Start-Sleep -Seconds 2 }",
    "build": "npm run build --workspace=src",
    "demo": "cd demo/ ; babel rollup.config.js --out-file rollup.config.mjs; Remove-Item dist -Recurse -ErrorAction Ignore; rollup --config"
  },
  "keywords": [],
  "author": "Fabrice Sanga (https://github.com/sangafabrice)",
  "license": "ISC",
  "type": "module",
  "workspaces": [
    "build",
    "src"
  ],
  "devDependencies": {
    "babel-plugin-syntax-hermes-parser": "^0.33.1",
    "flow-bin": "^0.295.0",
    "rollup-plugin-minify-template": "file:lib"
  }
}
